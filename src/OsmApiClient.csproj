<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>OsmApiClient</PackageId>
    <Version>0.0.1-alpha.1</Version>
    <RepositoryUrl>https://github.com/blackboxlogic/OsmApiClient</RepositoryUrl>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageProjectUrl />
    <Description>This is a simple C# client to allow using OSM API easily.</Description>
    <PackageTags>osm openstreetmap client</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="OAuth.DotNetCore" Version="3.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\core\src\OsmSharp\OsmSharp.csproj" />
  </ItemGroup>

</Project>
